<?xml version="1.0"?>
<joystick-profile>
  <name>Sony PLAYSTATION(R)3 Controller</name>
  <name>My Profile</name>
  <joystick>
    <calibration>
      <axis>
        <axis>ABS_X</axis>
        <precision>0</precision>
        <invert>1</invert>
        <center-min>-8</center-min>
        <center-max>-8</center-max>
        <range-min>-128</range-min>
        <range-max>-127</range-max>
      </axis>
      <axis>
        <axis>ABS_Y</axis>
        <precision>0</precision>
        <invert>1</invert>
        <center-min>-8</center-min>
        <center-max>-8</center-max>
        <range-min>-128</range-min>
        <range-max>-127</range-max>
      </axis>
    </calibration>
    <axis-map>
      <axis>ABS_X</axis>
      <axis>ABS_Y</axis>
    </axis-map>
    <button-map>
      <button>BTN_X</button>
      <button>BTN_Y</button>
    </button-map>
  </joystick>
</joystick-profile>
